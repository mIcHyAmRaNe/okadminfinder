#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.10.0 at
# Mon, 08 Jul 2024 00:28:22 +0200 and edited to use poetry

%:
	dh $@ --with python3 --buildsystem=pybuild

override_dh_auto_clean:
	poetry run python3 -m pip uninstall -y okadminfinder
	find . -name '*.pyc' -delete
	dh_auto_clean

override_dh_auto_build:
	poetry build

override_dh_auto_install:
	poetry run pip3 install . --no-deps --force --root=debian/okadminfinder --no-compile --prefix=/usr
	# poetry install --no-root --only main --no-interaction --no-ansi

override_dh_installman:
	dh_installman debian/res/okadminfinder.1

override_dh_python3:
	dh_python3
